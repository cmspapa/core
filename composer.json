{
    "name": "cmspapa/core",
    "description": "The cmspapa core.",
    "keywords": ["cms", "core", "papa", "cmspapa"],
    "license": "MIT",
    "support": {
        "issues": "https://github.com/cmspapa/core/issues",
        "source": "https://github.com/cmspapa/core"
    },
    "authors": [
        {
            "name": "Mahmoud Helal",
            "email": "mahmoudeid2050@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.6.4",
        "laravel/framework": "5.4.*",
        "laravel/tinker": "~1.0"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~5.7"
    },
    "autoload": {
        "psr-4": {
            "Cmspapa\\Core\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
